    "def checar_carroes(hands, N):\n",
    "    for player in hands:\n",
    "        count = 0\n",
    "        for index in range(7):\n",
    "            if hands[player][index][0]==hands[player][index][1]:\n",
    "                count += 1\n",
    "            if count == 4:\n",
    "                return False\n",
    "            else:\n",
    "                pass\n",
    "    return True"
